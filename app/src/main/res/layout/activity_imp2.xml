<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@drawable/custom25"
    android:layout_height="match_parent"
    tools:context=".imp2">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:layout_weight="1">
        <TextView
            android:id="@+id/css"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="CSE"
            android:textAlignment="center"
            android:gravity="center_horizontal"
            android:layout_marginTop="20dp"
            android:textStyle="bold"
            android:textSize="30dp"/>
        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:id="@+id/srl">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">
                <Button
                    android:id="@+id/btn910"
                    android:layout_below="@+id/css"
                    android:layout_width="match_parent"
                    android:layout_height="70dp"
                    android:layout_marginTop="40dp"
                    android:text="SEM3"
                    android:textStyle="bold"
                    android:textSize="30dp"
                    android:background="@drawable/custom_button1"
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="30dp"/>
                <Button
                    android:id="@+id/btn920"
                    android:layout_below="@+id/btn99"
                    android:layout_width="match_parent"
                    android:layout_height="70dp"
                    android:layout_marginTop="40dp"
                    android:text="SEM4"
                    android:textStyle="bold"
                    android:textSize="30dp"
                    android:background="@drawable/custom_button1"
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="30dp"/>
                <Button
                    android:id="@+id/btn930"
                    android:layout_below="@+id/btn100"
                    android:layout_width="match_parent"
                    android:layout_height="70dp"
                    android:layout_marginTop="40dp"
                    android:text="SEM5"
                    android:textStyle="bold"
                    android:textSize="30dp"
                    android:background="@drawable/custom_button1"
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="30dp"/>
                <Button
                    android:id="@+id/btn940"
                    android:layout_below="@+id/btn101"
                    android:layout_width="match_parent"
                    android:layout_height="70dp"
                    android:layout_marginTop="40dp"
                    android:text="SEM6"
                    android:textStyle="bold"
                    android:textSize="30dp"
                    android:background="@drawable/custom_button1"
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="30dp"/>
                <Button
                    android:id="@+id/btn950"
                    android:layout_below="@+id/btn102"
                    android:layout_width="match_parent"
                    android:layout_height="70dp"
                    android:layout_marginTop="40dp"
                    android:text="SEM7"
                    android:textStyle="bold"
                    android:textSize="30dp"
                    android:background="@drawable/custom_button1"
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="30dp"/>
                <Button
                    android:id="@+id/btn960"
                    android:layout_below="@+id/btn103"
                    android:layout_width="match_parent"
                    android:layout_height="70dp"
                    android:layout_marginTop="40dp"
                    android:text="SEM8"
                    android:background="@drawable/custom_button1"
                    android:textStyle="bold"
                    android:textSize="30dp"
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="30dp"/>


            </LinearLayout>
        </ScrollView>
    </LinearLayout>

</LinearLayout>